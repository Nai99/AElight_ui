{
  "namespace": "animations_mui",
  
  /////////////////////
  //TITLE_ANIMATIONS//
  ///////////////////
  
  "title_wait": {
    "anim_type": "alpha",
    "duration": 0.75,
    "from": 1,
    "to": 0
  },
  
  "title_exit_animation_push": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": "$transition_time_push",
    "from": [ 0, 2 ],
    "to": [ 0, "-100%" ],
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },

  "title_exit_animation_pop": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": "$transition_time_pop",
    "from": [ 0, 2 ],
    "to": [ 0, "-100%" ],
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },

  "title_entrance_animation_push": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": "$transition_time_push",
    "from": [ 0, "-100%" ],
    "to": [ 0, 2 ],
    "play_event": "screen.entrance_push",
    "end_event": "screen.entrance_end"
  },

  "title_entrance_animation_pop": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": "$transition_time_pop",
    "from": [ 0, "-100%" ],
    "to": [ 0, 2 ],
    "play_event": "screen.entrance_pop",
    "end_event": "screen.entrance_end"
  },
  
